<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.moreds.dao.UserMapper">
    <select id="findOne" resultType="com.example.moreds.bean.User">
        SELECT ID AS id, USER_NAME AS userName, CREATE_TIME AS createTime, DATA as data
        FROM db1.user
        WHERE ID = 1
    </select>
</mapper>